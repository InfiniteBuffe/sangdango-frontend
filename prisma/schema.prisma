generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model RentalLog {
  id        Int      @id @default(autoincrement())
  name      String   @db.VarChar(10)
  studentId Int
  createdAt DateTime @default(now())
}

model CurrentRental {
  id        Int      @id @default(autoincrement())
  name      String   @db.VarChar(10)
  studentId Int
  createdAt DateTime @default(now())
}

model User {
  id        Int      @id @default(autoincrement())
  kakao_id  String   @unique
  admin     Boolean  @default(false)
  createdAt DateTime @default(now())
}

model Settings {
  id        Int      @id @default(autoincrement())
  service   String
  name      String   @unique
  value     String
  updatedAt DateTime
}
